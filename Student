public class Student extends Person {
  public String studentID;
  public int academic_year;

  public Student(String name,char gender, int birth_year, String studentID, int academic_year) {
    super(name, gender, birth_year);
    this.setStudentID(studentID);
    this.setAcademic_year(academic_year);
  }
  
  public String getStudentID() {
    return studentID;
  }

  public void setStudentID(String studentID) {
    this.studentID = studentID;
  }

  public int getAcademic_year() {
    return academic_year;
  }

  public void setAcademic_year(int academic_year) {
    this.academic_year = academic_year;
  }

  @Override
  public String toString() {
    return "Student [ Name = " + getName() + ", Gender = " + getGender() + ", Birth_year = "
        + getBirth_year() + ", studentID = " + studentID + ", academic_year = " + academic_year + "]";
  }
  
  
}
